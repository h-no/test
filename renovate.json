{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],

  "packageRules": [
    {
      "matchPackagePatterns": ["!^github.com/testcontainers.*"],
      "schedule": ["every tuesday"]
    },
    {
      "matchPackagePatterns": ["!^github.com/testcontainers.*"],
      "schedule": ["at any time"]
    }
  ]
}
